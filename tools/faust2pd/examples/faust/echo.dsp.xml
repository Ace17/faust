
<faust>
	<name>echo</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>1</inputs>
	<outputs>1</outputs>
	<ui>
		<activewidgets>
			<count>2</count>
			<widget type="hslider" id="1">
				<label>feedback</label>
				<varname>fslider1</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>1e+02f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>millisecond</label>
				<varname>fslider0</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>1e+03f</max>
				<step>0.1f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>echo-simple</label>
				<group type="vgroup">
					<label>echo  1000</label>
					<widgetref id="1" />
					<widgetref id="2" />
				</group>
			</group>
		</layout>
	</ui>
</faust>
