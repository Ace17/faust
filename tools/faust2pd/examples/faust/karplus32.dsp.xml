
<faust>
	<name>karplus32</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>0</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>8</count>
			<widget type="hslider" id="1">
				<label>excitation (samples)</label>
				<varname>fslider1</varname>
				<init>128.0f</init>
				<min>2.0f</min>
				<max>512.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="button" id="2">
				<label>play</label>
				<varname>fbutton0</varname>
			</widget>
			<widget type="hslider" id="3">
				<label>level</label>
				<varname>fslider2</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>output volume</label>
				<varname>fslider6</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="5">
				<label>attenuation</label>
				<varname>fslider0</varname>
				<init>0.1f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="6">
				<label>detune</label>
				<varname>fslider3</varname>
				<init>32.0f</init>
				<min>0.0f</min>
				<max>512.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="7">
				<label>duration (samples)</label>
				<varname>fslider4</varname>
				<init>128.0f</init>
				<min>2.0f</min>
				<max>512.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="8">
				<label>polyphony</label>
				<varname>fslider5</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>32.0f</max>
				<step>1.0f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>karplus32</label>
				<group type="vgroup">
					<label>excitator</label>
					<widgetref id="1" />
					<widgetref id="2" />
				</group>
				<group type="vgroup">
					<label>noise generator</label>
					<widgetref id="3" />
				</group>
				<widgetref id="4" />
				<group type="vgroup">
					<label>resonator x32</label>
					<widgetref id="5" />
					<widgetref id="6" />
					<widgetref id="7" />
					<widgetref id="8" />
				</group>
			</group>
		</layout>
	</ui>
</faust>
