
<faust>
	<name>lowboost</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>1</inputs>
	<outputs>1</outputs>
	<ui>
		<activewidgets>
			<count>2</count>
			<widget type="hslider" id="1">
				<label>freq</label>
				<varname>fslider0</varname>
				<init>1e+03f</init>
				<min>2e+01f</min>
				<max>2e+04f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>gain (db)</label>
				<varname>fslider1</varname>
				<init>0.0f</init>
				<min>-2e+01f</min>
				<max>2e+01f</max>
				<step>0.1f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>low-freq shelving boost</label>
				<widgetref id="1" />
				<widgetref id="2" />
			</group>
		</layout>
	</ui>
</faust>
