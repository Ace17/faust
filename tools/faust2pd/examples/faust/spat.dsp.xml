
<faust>
	<name>spat</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>1</inputs>
	<outputs>8</outputs>
	<ui>
		<activewidgets>
			<count>2</count>
			<widget type="hslider" id="1">
				<label>angle</label>
				<varname>fslider1</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>distance</label>
				<varname>fslider0</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>Spatializer 1x8</label>
				<widgetref id="1" />
				<widgetref id="2" />
			</group>
		</layout>
	</ui>
</faust>
