
<faust>
	<name>tester</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>0</inputs>
	<outputs>8</outputs>
	<ui>
		<activewidgets>
			<count>6</count>
			<widget type="hslider" id="1">
				<label>(1) freq</label>
				<varname>fslider0</varname>
				<init>1e+03f</init>
				<min>0.0f</min>
				<max>2.4e+04f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>(2)volume</label>
				<varname>fslider1</varname>
				<init>-96.0f</init>
				<min>-96.0f</min>
				<max>0.0f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>(3) destination</label>
				<varname>fslider2</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>8.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="checkbox" id="4">
				<label>pink noise</label>
				<varname>fcheckbox0</varname>
			</widget>
			<widget type="checkbox" id="5">
				<label>sine wave</label>
				<varname>fcheckbox2</varname>
			</widget>
			<widget type="checkbox" id="6">
				<label>white noise</label>
				<varname>fcheckbox1</varname>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>Audio Tester</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
				<widgetref id="4" />
				<widgetref id="5" />
				<widgetref id="6" />
			</group>
		</layout>
	</ui>
</faust>
