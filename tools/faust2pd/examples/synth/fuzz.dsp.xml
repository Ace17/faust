
<faust>
	<name>fuzz -- a simple distortion effect</name>
	<author>Bram de Jong (from musicdsp.org)</author>
	<copyright></copyright>
	<license></license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>2</count>
			<widget type="hslider" id="1">
				<label>distortion</label>
				<varname>fslider0</varname>
				<init>12.0f</init>
				<min>0.0f</min>
				<max>1e+02f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>gain</label>
				<varname>fslider1</varname>
				<init>3.0f</init>
				<min>-96.0f</min>
				<max>96.0f</max>
				<step>0.1f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>dist</label>
				<widgetref id="1" />
				<widgetref id="2" />
			</group>
		</layout>
	</ui>
</faust>
