
<faust>
	<name>expander -- expander/noise gate unit</name>
	<author>Albert Graef</author>
	<copyright></copyright>
	<license></license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>5</count>
			<widget type="nentry" id="1">
				<label>knee</label>
				<varname>fentry1</varname>
				<init>3.0f</init>
				<min>0.0f</min>
				<max>2e+01f</max>
				<step>0.1f</step>
			</widget>
			<widget type="nentry" id="2">
				<label>ratio</label>
				<varname>fentry2</varname>
				<init>2.0f</init>
				<min>1.0f</min>
				<max>2e+01f</max>
				<step>0.1f</step>
			</widget>
			<widget type="nentry" id="3">
				<label>threshold</label>
				<varname>fentry0</varname>
				<init>-4e+01f</init>
				<min>-96.0f</min>
				<max>1e+01f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>attack</label>
				<varname>fslider1</varname>
				<init>0.001f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="5">
				<label>release</label>
				<varname>fslider0</varname>
				<init>0.1f</init>
				<min>0.0f</min>
				<max>1e+01f</max>
				<step>0.01f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>1</count>
			<widget type="hbargraph" id="6">
				<label>gain</label>
				<varname>fbargraph0</varname>
				<min>-96.0f</min>
				<max>0.0f</max>
			</widget>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label></label>
				<group type="hgroup">
					<label>1-expander</label>
					<widgetref id="1" />
					<widgetref id="2" />
					<widgetref id="3" />
				</group>
				<group type="vgroup">
					<label>2-envelop</label>
					<widgetref id="4" />
					<widgetref id="5" />
				</group>
				<group type="vgroup">
					<label>3-gain</label>
					<widgetref id="6" />
				</group>
			</group>
		</layout>
	</ui>
</faust>
