
<faust>
	<name>echo -- stereo delay effect</name>
	<author>Albert Graef</author>
	<copyright></copyright>
	<license></license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>4</count>
			<widget type="hslider" id="1">
				<label>delay</label>
				<varname>fslider2</varname>
				<init>0.04f</init>
				<min>0.0f</min>
				<max>5.0f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>feedback</label>
				<varname>fslider1</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>level</label>
				<varname>fslider3</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>stereo</label>
				<varname>fslider0</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.001f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>echo</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
				<widgetref id="4" />
			</group>
		</layout>
	</ui>
</faust>
