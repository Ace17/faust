
<faust>
	<name>karplus -- Karplus-Strong string synth</name>
	<author>Yann Orlarey</author>
	<copyright></copyright>
	<license></license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>0</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>8</count>
			<widget type="nentry" id="1">
				<label>gain</label>
				<varname>fentry0</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1e+01f</max>
				<step>0.01f</step>
			</widget>
			<widget type="button" id="2">
				<label>gate</label>
				<varname>fbutton0</varname>
			</widget>
			<widget type="hslider" id="3">
				<label>samples</label>
				<varname>fslider0</varname>
				<init>512.0f</init>
				<min>1.0f</min>
				<max>1024.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>decay time</label>
				<varname>fslider2</varname>
				<init>4.0f</init>
				<min>0.0f</min>
				<max>1e+01f</max>
				<step>0.01f</step>
			</widget>
			<widget type="nentry" id="5">
				<label>freq</label>
				<varname>fentry1</varname>
				<init>4.4e+02f</init>
				<min>2e+01f</min>
				<max>2e+04f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="6">
				<label>pitch bend</label>
				<varname>fslider1</varname>
				<init>0.0f</init>
				<min>-2.0f</min>
				<max>2.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="7">
				<label>pan</label>
				<varname>fslider3</varname>
				<init>0.0f</init>
				<min>-1.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="8">
				<label>vol</label>
				<varname>fslider4</varname>
				<init>-1e+01f</init>
				<min>-96.0f</min>
				<max>96.0f</max>
				<step>0.1f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label></label>
				<group type="vgroup">
					<label>1-excitator</label>
					<widgetref id="1" />
					<widgetref id="2" />
					<widgetref id="3" />
				</group>
				<group type="vgroup">
					<label>2-resonator</label>
					<widgetref id="4" />
					<widgetref id="5" />
					<widgetref id="6" />
				</group>
				<group type="vgroup">
					<label>3-master</label>
					<widgetref id="7" />
					<widgetref id="8" />
				</group>
			</group>
		</layout>
	</ui>
</faust>
